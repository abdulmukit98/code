#include<bits/stdc++.h>
using namespace std;
#define null NULL
#define ll long long
void insertion(int* arr, int n);

int main()
{
    /***
        lightOJ 1227

    */

    int test; scanf("%d",&test);
    for(int t=1; t<=test; t++)
    {
        int no, limitP, mxWeightQ;
        scanf("%d %d %d",&no, &limitP, &mxWeightQ);

        int weight[no];
        for(int i=0; i<no; i++) scanf("%d",&weight[i]);
        insertion(weight, no);

        int wei = 0, cnt=0;
        for(int i=0; i<limitP; i++)
        {
            wei = wei + weight[i];
            cnt = cnt+1;
            if(wei > mxWeightQ)
            {
                wei = wei - weight[i];
                cnt = cnt - 1;
                break;
            }
        }

    printf("Case %d: %d\n",t,cnt);
    }


    return 0;
}
void insertion(int* arr, int n)
{
    for(int j=1; j<n; j++)
    {
        int key = arr[j];
        int i = j-1;
        while(arr[i]>key && i>=0)
        {
            arr[i+1] = arr[i];
            i = i-1;
        }
        arr[i+1] = key;
    }
}

